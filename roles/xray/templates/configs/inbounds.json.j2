{
  "inbounds": [
    {
      "listen": "{{ ansible_default_ipv4['address'] }}",
      "port": {{ listen_port }},
      "protocol": "vless",
      "tag": "reality-in",
      "settings": {
        "clients": [
{% for client, uuid in clients_uuids.items() %}
          {
            "id": "{{ uuid }}",
            "email": "{{ client }}",
            "flow": "xtls-rprx-vision"
{% if loop.last %}
          }
{% else %}
          },
{% endif %}
{% endfor %}
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "tcp",
        "security": "reality",
        "realitySettings": {
          "show": false,
          "dest": "{{ mask_domain }}:443",
          "xver": 0,
          "serverNames": [
            "{{ mask_domain }}"
          ],
          "privateKey": "{{ priv_key }}",
          "minClientVer": "",
          "maxClientVer": "",
          "maxTimeDiff": 0,
          "shortIds": [""]
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls",
          "quic"
        ]
      }
    },
    {
      "listen": "127.0.0.1",
      "port": 11111,
      "protocol": "dokodemo-door",
      "tag": "metrics_in",
      "settings": {
        "address": "127.0.0.1"
      }
    },
    {
      "listen": "127.0.0.1",
      "port": 9999,
      "protocol": "dokodemo-door",
      "tag": "api",
      "settings": {
        "address": "127.0.0.1"
      }
    }
  ]
}